package Model;

public class Server {
	
	private int id, slots, capacity, xCoordinate;
	
	// Constructor.
	public Server() {

	}
	
	// Setters and getters.
	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public int getSlots() {
		return slots;
	}

	public void setSlots(int slots) {
		this.slots = slots;
	}

	public int getCapacity() {
		return capacity;
	}

	public void setCapacity(int capacity) {
		this.capacity = capacity;
	}

	public int getxCoordinate() {
		return xCoordinate;
	}

	public void setxCoordinate(int xCoordinate) {
		this.xCoordinate = xCoordinate;
	}
}
